									"    .tftable tr:hover {background-color:#e0ffff;}\r",
									"</style>\r",
									"\r",
									"<table class=\"tftable\" border=\"1\">\r",
									"    <tr>\r",
									"        <th>Order ID</th>\r",
									"        <th>Vehicle ID</th>\r",
									"        <th>Driver ID</th>\r",
									"        <th>Start Date</th>\r",
									"        <th>End Date</th>\r",
									"        <th>Start Time</th>\r",
									"        <th>End Time</th>\r",
									"        <th>Destination</th>\r",
									"        <th>Odometer Before</th>\r",
									"        <th>Odometer After</th>\r",
									"    </tr>\r",
									"    \r",
									"    {{#each response.$values}}\r",
									"        <tr>\r",
									"            <td>{{orderId}}</td>\r",
									"            <td>{{vehicleId}}</td>\r",
									"            <td>{{driverId}}</td>\r",
									"            <td>{{startDate}}</td>\r",
									"            <td>{{endDate}}</td>\r",
									"            <td>{{startTime}}</td>\r",
									"            <td>{{endTime}}</td>\r",
